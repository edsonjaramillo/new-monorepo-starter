{
  "name": "@repo/context",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "exports": {
    "./auth-popup": {
      "types": "./dist/auth-popup.context.d.ts",
      "import": "./dist/auth-popup.context.mjs"
    },
    "./menu": {
      "types": "./dist/menu.context.d.ts",
      "import": "./dist/menu.context.mjs"
    },
    "./session": {
      "types": "./dist/session.context.d.ts",
      "import": "./dist/session.context.mjs"
    }
  },
  "scripts": {
    "build": "tsup --env.CONFIG=production",
    "check-types": "tsc --noEmit",
    "clean": "rimraf dist",
    "clean-cache": "rimraf .turbo",
    "clean-node-modules": "rimraf node_modules",
    "dev": "tsup --env.CONFIG=development",
    "format": "prettier --write . && eslint . --fix",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "prep": "tsup --env.CONFIG=prepare"
  },
  "dependencies": {
    "@repo/database": "workspace:*",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.11.0",
    "@repo/configs": "workspace:*",
    "@types/node": "22.13.5",
    "eslint": "9.21.0",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "rimraf": "6.0.1",
    "tsup": "8.3.6",
    "typescript": "5.7.3"
  }
}
