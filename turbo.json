{
  "$schema": "./node_modules/turbo/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "DEBUG",
        "PORT",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_NAME",
        "POSTGRES_PASSWORD",
        "POSTGRES_PORT",
        "POSTGRES_USER",
        "REDIS_HOST",
        "REDIS_PASSWORD",
        "REDIS_PORT",
        "REDIS_DATABASE",
        "REDIS_SKIP_CACHE"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "clean": {
      "cache": false
    },
    "check-types": {
      "cache": true
    },
    "clean-cache": {
      "cache": false
    },
    "clean-node-modules": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "seed": {
      "cache": false
    },
    "pre-dev": {
      "dependsOn": ["^pre-dev"],
      "cache": true
    },
    "start": {
      "cache": false
    },
    "studio": {
      "cache": false
    }
  }
}
