{
  "$schema": "./node_modules/turbo/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "DEBUG",
        "PORT",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_NAME",
        "POSTGRES_PASSWORD",
        "POSTGRES_PORT",
        "POSTGRES_USER",
        "REDIS_HOST",
        "REDIS_PASSWORD",
        "REDIS_PORT",
        "REDIS_DATABASE",
        "JWT_SECRET",
        "JWT_ISSUER",
        "JWT_SUBJECT",
        "JWT_ALGORITHM"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".output/**", ".vinxi/**"]
    },
    "prep": {
      "dependsOn": ["^prep"],
      "env": [
        "NODE_ENV",
        "DEBUG",
        "PORT",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_NAME",
        "POSTGRES_PASSWORD",
        "POSTGRES_PORT",
        "POSTGRES_USER",
        "REDIS_HOST",
        "REDIS_PASSWORD",
        "REDIS_PORT",
        "REDIS_DATABASE",
        "JWT_SECRET",
        "JWT_ISSUER",
        "JWT_SUBJECT",
        "JWT_ALGORITHM"
      ],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "check-types": {
      "cache": true
    },
    "clean-cache": {
      "cache": false
    },
    "clean-node-modules": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "lint:fix": {
      "cache": false
    },
    "seed": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "studio": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"],
      "cache": true
    }
  }
}
